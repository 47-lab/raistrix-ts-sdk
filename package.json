{
  "name": "raistrix-ts-sdk",
  "version": "1.0.0",
  "description": "This is the typescript sdk for interacting with the raistrix.com api",
  "license": "ISC",
  "author": "Lennard Tappe",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/esm/index.js",
  "source": "./src/index.ts",
  "scripts": {
    "test": "vitest",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "zod": "^4.2.1"
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "keywords": [
    "sdk",
    "typescript",
    "ts",
    "raistrix"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/47-lab/raistrix-ts-sdk"
  }
}
